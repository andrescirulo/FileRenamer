<UserControl x:Class="FilaSubtitulo"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:FileRenamer"
             mc:Ignorable="d" 
             HorizontalAlignment="Stretch" >
    <Grid>
        <CheckBox Name="chkElegido" Content="CheckBox" HorizontalAlignment="Stretch" Margin="5,5,5,0" VerticalAlignment="Top"/>
        <Label Name="lblSubtitulo" Content="lblSubtitulo" HorizontalAlignment="Stretch"  Margin="35,20,5,0" VerticalAlignment="Top"/>
    </Grid>
</UserControl>
